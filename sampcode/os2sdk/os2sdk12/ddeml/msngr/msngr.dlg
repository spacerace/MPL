        CONTROL "5", IDC_EF_SPEED, 167, 8, 18, 8, WC_ENTRYFIELD, ES_CENTER | 
                ES_MARGIN | WS_TABSTOP | WS_VISIBLE
        CONTROL "~Send", IDC_SENDSONG, 8, 6, 63, 12, WC_BUTTON, BS_PUSHBUTTON | 
