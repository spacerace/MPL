DLGTEMPLATE IDD_SENDTEXT LOADONCALL MOVEABLE DISCARDABLE 
BEGIN
    DIALOG "Send Text Message", IDD_SENDTEXT, 10, 20, 184, 50, FS_NOBYTEALIGN | 
                FS_DLGBORDER | WS_VISIBLE | WS_CLIPSIBLINGS | WS_SAVEBITS, 
                FCF_TITLEBAR
    BEGIN
        CONTROL "", IDC_EF_TEXT, 14, 32, 158, 8, WC_ENTRYFIELD, 
                ES_LEFT | ES_AUTOSCROLL | ES_MARGIN | WS_TABSTOP | WS_VISIBLE
        CONTROL "Cancel", 2, 104, 10, 70, 12, WC_BUTTON, BS_PUSHBUTTON | 
                WS_TABSTOP | WS_VISIBLE
        CONTROL "Send", IDC_SENDTEXT, 12, 10, 70, 12, WC_BUTTON, BS_PUSHBUTTON | 
                BS_DEFAULT | WS_TABSTOP | WS_VISIBLE
    END
END

