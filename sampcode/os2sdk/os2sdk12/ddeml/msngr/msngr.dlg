                BS_PUSHBUTTON | WS_TABSTOP | WS_VISIBLE | WS_DISABLED
        CONTROL "Erase ~all", IDC_ERASEALL, 200, 24, 62, 12, WC_BUTTON, 
                BS_PUSHBUTTON | WS_TABSTOP | WS_VISIBLE | WS_DISABLED
        CONTROL "~Erase note", IDC_ERASENOTE, 200, 42, 62, 12, WC_BUTTON, 
                BS_PUSHBUTTON | WS_TABSTOP | WS_VISIBLE | WS_DISABLED
